{
  "name": "maineffect",
  "version": "0.0.17",
  "description": "Writing tests by redacting instead of mocking.",
  "main": "dist/maineffect.js",
  "scripts": {
    "test": "nyc mocha test/**/*.test.js --require @babel/register",
    "test-dev": "mocha --timeout 10000  --require @babel/register --watch",
    "build": "npx webpack --config webpack.config.js"
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ],
  "keywords": [
    "unit",
    "test",
    "mock",
    "left"
  ],
  "author": "arvind.raj.naidu@gmail.com",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-transform-spread": "^7.8.3",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.8.3",
    "@babel/register": "^7.4.0",
    "babel-loader": "^8.0.5",
    "istanbul-api": "^2.1.1",
    "traverse": "^0.6.6"
  },
  "devDependencies": {
    "babel-plugin-istanbul": "^6.0.0",
    "chai": "^4.2.0",
    "mocha": "^7.0.1",
    "nodemon": "1.12.0",
    "nyc": "^13.3.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-test-renderer": "^16.12.0",
    "sinon": "^7.4.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "nyc": {
    "all": true,
    "cache": false,
    "sourceMap": false,
    "instrument": false,
    "include": [
      "src/examples/**/*.js"
    ],
    "exclude": [
      "**/**.test.js"
    ]
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arvindrajnaidu/maineffect.git"
  },
  "bugs": {
    "url": "https://github.com/arvindrajnaidu/maineffect/issues"
  },
  "homepage": "https://github.com/arvindrajnaidu/maineffect#readme"
}
